{
  "name": "middlebury-design-system",
  "version": "0.12.0",
  "description": "Front-end design system for building Middlebury websites",
  "keywords": [
    "components",
    "css",
    "design-system",
    "framework",
    "javascript",
    "js",
    "sass",
    "scss",
    "styleguide",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/middlebury/middlebury-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/middlebury/middlebury-design-system/issues"
  },
  "license": "GPL-3.0",
  "main": "umd/index.js",
  "module": "es/index.js",
  "style": "dist/css/mds.css",
  "sass": "scss/mds.scss",
  "maintainers": [
    {
      "name": "Zeb Pykosz",
      "email": "zpykosz@middlebury.edu"
    }
  ],
  "homepage": "https://design.middlebury.edu/",
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "dist/",
    "scss/",
    "es/",
    "umd/"
  ],
  "scripts": {
    "start": "gulp dev",
    "build": "gulp build",
    "clean": "gulp clean",
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "install:site": "cd site && yarn",
    "build:site": "cd site && yarn build",
    "netlify": "yarn gulp buildIcons && yarn install:site && yarn build:site",
    "test": "bundlesize"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.7",
    "bundlesize": "^0.18.0",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-combine-mq": "^0.4.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-dom": "^0.9.17",
    "gulp-if": "^2.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgo": "^1.5.4",
    "gulp-uglify": "^3.0.2",
    "gulp-wrap": "^0.13.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/css/*.css",
      "maxSize": "10 kB"
    },
    {
      "path": "./dist/js/*.js",
      "maxSize": "10 kB"
    }
  ],
  "resolutions": {
    "lodash": "^4.17.14",
    "minimatch": "^3.0.2",
    "js-yaml": "^3.13.0",
    "braces": "^2.3.1",
    "set-value": "^3.0.1",
    "mixin-deep": "^2.0.1",
    "clean-css": "^4.1.11"
  }
}
